13) public void Entrar() {
       String login, senha;
       String login1 = "Gabriela";
       String senha1 = "Teste@123";
       
System.out.println("Entre com seu Login");
   login = tec.nextLine();
   System.out.println("------------------");

 if (login.equals(login1) && senha.equals(senha1)) {
       Media();
   } else {
       System.out.println("Entrada Proibida");
   }

double salarioFico;

System.out.println("Digite o salário fixo do vendedor:  ");
double  salarioFixo = tec.nextDouble();
System.out.print("Digite o valor total das vendas:   ");
double valorVendas = tec.nextDouble();

double comissao = 0;
            if (valorVendas <= 1500) {
   comissao = valorVendas * 0.03;
            } else {
 comissao = (1500 * 0.03) + (valorVendas - 1500) * 0.05;
            }

 double salarioTotal = salarioFixo + comissao;
 System.out.printf("O salário total do vendedor é: R$ %.2f", salarioTotal);

14) public void Entrar() {
       String login, senha;
       String login1 = "Gabriela";
       String senha1 = "Teste@123";
       
System.out.println("Entre com seu Login");
   login = tec.nextLine();
   System.out.println("------------------");

 if (login.equals(login1) && senha.equals(senha1)) {
       Media();
   } else {
       System.out.println("Entrada Proibida");
   }

double salarioFico;

System.out.println("Digite o salário fixo do vendedor:  ");
double  salarioFixo = tec.nextDouble();
System.out.print("Digite o valor total das vendas:   ");
double valorVendas = tec.nextDouble();
double comissao = 0;
            if (valorVendas <= 1500) {
                comissao = valorVendas * 0.03;
            } else {
                comissao = (1500 * 0.03) + (valorVendas - 1500) * 0.05;
            }
double salarioTotal = salarioFixo + comissao;  
System.out.printf("O salário total do vendedor é: R$ %.2f", salarioTotal);



