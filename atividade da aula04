1) public void Entrar() {
       String login, senha;
       String login1 = "Gabriela";
       String senha1 = "Teste@123";
       
System.out.println("Entre com seu Login");
   login = tec.nextLine();
   System.out.println("------------------");

System.out.println("Entre com sua Senha");
senha = tec.nextLine();

   if (login.equals(login1) && senha.equals(senha1)) {
       Media();
   } else {
       System.out.println("Entrada Proibida");
   }

   }
   public void Media() {
       double n1, n2, media1;
       System.out.println("Entre com N1");
       n1 = tec.nextDouble();
       System.out.println("Entre com N2");
       n2 = tec.nextDouble();
       media1 = (n1+n2)/ 2;
       Sistema.out.println("---------");
       Sistema.out.println(media1);
       Sistema.out.println("---------");
       se (media1 > 7) {
           System.out.println("Aprovado");
           Media4();
       } else {

           System.out.println("Reprovado");
       }
       
   }

}

